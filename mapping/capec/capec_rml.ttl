@prefix ucoex: <http://example.com/uco-extended#> .
@base <http://purl.org/cyber/uco#> .

#CVE Class mapping
<#CAPECMap> a rr:TriplesMap;
             rml:logicalSource [
                   rml:source "./data/capec/capec.json";
                   rml:referenceFormulation ql:JSONPath;
                   rml:iterator "$[*]"
               ];
             rr:predicateObjectMap [
                   rr:predicate ucoex:ucohasRelatedWeakness;
                   rr:objectMap [
                                    rr:template "http://purl.org/cyber/uco#{.Related_Weaknesses[*]}";
                                    rr:class uco:ucoCWE
                                ]
               ];
            rr:predicateObjectMap [
                                      rr:predicate ucoex:ucohasTaxonomyMapping ;
                                      rr:objectMap [
                                                       rr:template "http://example.com/uco-extended#{.Taxonomy_Mappings[*]}";
                                                       rr:class ucoex:ucoMITREATTACK
                                                   ]
                                  ];
             rr:subjectMap [
                   rr:template "http://example.com/uco-extended#CAPEC-{ID}";
                   rr:class ucoex:ucoMITRED3FEND
               ].
